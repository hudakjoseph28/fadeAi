# Helius API Key for Solana transaction data
HELIUS_API_KEY=

# Solscan API Token for token metadata
SOLSCAN_API_TOKEN=

# Price provider (birdeye or dexscreener)
PRICE_PROVIDER=

# Birdeye API Key (if using birdeye as price provider)
BIRDEYE_API_KEY=


# Database URL for Prisma
DATABASE_URL="file:./dev.db"

# Logging Controls (optional)
# Set to "true" to enable verbose transaction-level P&L logging
VERBOSE=false

# Set to "true" to show price lookup warnings
DEBUG_PRICES=false

# Set to "true" to save structured JSON analysis to analysis.json file
SAVE_ANALYSIS_JSON=false
